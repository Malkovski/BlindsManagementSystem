@model Blinds.Web.Areas.Administration.Models.BlindTypesModel
@using Kendo.Mvc.UI
<div class="kendo-form">
    @using (Html.BeginForm())
    {
        <div class="form-horizontal">
            <fieldset>
                @Html.HiddenFor(m => m.Id)
                @Html.HiddenFor(m => m.HasImage)

                @Html.EditorFor(m => m.Name)
                @Html.EditorFor(m => m.Price)
                @Html.EditorFor(m => m.Info)
                @Html.EditorFor(m => m.File)
            </fieldset>
        </div>
    }
</div>




